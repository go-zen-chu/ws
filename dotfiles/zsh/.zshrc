#!/usr/bin/env zsh

source $HOME/dotfiles/zsh/zinit.zsh
source $HOME/dotfiles/zsh/alias.zsh
source $HOME/dotfiles/zsh/prompt.zsh
source $HOME/dotfiles/zsh/key.zsh
source $HOME/dotfiles/zsh/settings.zsh
